<%= form_for(@user) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %><br />
  <%= f.email_field :email, :autofocus => true %></div>

  <div><%= f.label :time_zone %><br />
  <%= f.time_zone_select :time_zone, ActiveSupport::TimeZone.us_zones %></div>

  <div><%= f.label :latitude %><br />
  <%= f.text_field :latitude %></div>

  <div><%= f.label :longitude %><br />
  <%= f.text_field :longitude %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>